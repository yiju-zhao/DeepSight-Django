# Frontend Development Configuration

# Enable HTTPS in development to resolve blob URL security warnings
# Set to 'true' to enable HTTPS, 'false' or omit to use HTTP
VITE_HTTPS=false

# Backend API URL
VITE_BACKEND_URL=http://localhost:8000

# API Base URL for the frontend
VITE_API_BASE_URL=http://localhost:8000/api

# Show React Query Devtools in development (floating icon/panel)
# Set to 'true' to enable, otherwise leave unset or 'false' to hide
VITE_SHOW_RQ_DEVTOOLS=false
